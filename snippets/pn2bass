-- @pn2bass
let p = slow 4 $ s "${1:bass}*2 $1*2 $1*3 $1*3" -- (3) slow 5
    -- p = slow 5 $ s "$1*2 $1*2 $1*3 $1*3 ~" -- (2)
  in d'bass $ p
